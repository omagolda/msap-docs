# sent_id = weblog-blogspot.com_tacitusproject_20040712123425_ENG_20040712_123425-0019
# text = (Applause.)

# sent_id = weblog-blogspot.com_tacitusproject_20040712123425_ENG_20040712_123425-0020
# newpar id = weblog-blogspot.com_tacitusproject_20040712123425_ENG_20040712_123425-p0002
# text = "And so we sang this hymn -- this is a long story trying to get to your answer.

# sent_id = weblog-blogspot.com_tacitusproject_20040712123425_ENG_20040712_123425-0021
# text = (Laughter.)

# sent_id = weblog-blogspot.com_tacitusproject_20040712123425_ENG_20040712_123425-0022
# text = This is not a filibuster.

# sent_id = weblog-blogspot.com_tacitusproject_20040712123425_ENG_20040712_123425-0023
# text = (Laughter.)

# sent_id = weblog-blogspot.com_tacitusproject_20040712123425_ENG_20040712_123425-0024
# text = That's a Senate term -- particularly on good judges.

# sent_id = weblog-blogspot.com_tacitusproject_20040712123425_ENG_20040712_123425-0025
# text = (Applause.)

# sent_id = weblog-blogspot.com_tacitusproject_20040712123425_ENG_20040712_123425-0026
# text = The hymn was sung at my first inaugural church service as governor.

# sent_id = weblog-blogspot.com_tacitusproject_20040712123425_ENG_20040712_123425-0027
# text = Laura and I are Methodists.

# sent_id = weblog-blogspot.com_tacitusproject_20040712123425_ENG_20040712_123425-0028
# text = One of the Wesley boys wrote the hymn.

# sent_id = weblog-blogspot.com_tacitusproject_20040712123425_ENG_20040712_123425-0029
